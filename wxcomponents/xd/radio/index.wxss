.radio{
	display: flex;
	align-items: center;
	margin-right: 50rpx;
}
.raiod-icon{
	width: 30rpx;
	height: 30rpx;
	border: 2rpx solid #979797;
	margin-right: 10rpx;
	border-radius: 50%;
}
.raiod-icon.checked{
	border-color: #3A85F5;
	
}
.circle{
	width: 22rpx;
	height: 22rpx;
	background: #3A85F5;
	border-radius: 50%;
	margin-left: 4rpx;
	margin-top:4rpx;
	display: block;
}
.label{
	color: #333;
	line-height: 50rpx;
	font-weight: bold;
	font-size: 36rpx;
}